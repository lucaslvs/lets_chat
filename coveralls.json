{
  "default_stop_words": [
    "defmodule",
    "defrecord",
    "defimpl",
    "def.+(.+\/\/.+).+do"
  ],
  "skip_files": [
    "test",
    "priv",
    "lib/lets_chat_web.ex",
    "lib/lets_chat/release.ex"
  ],
  "coverage_options": {
    "treat_no_relevant_lines_as_covered": true,
    "output_dir": "cover/"
  }
}
